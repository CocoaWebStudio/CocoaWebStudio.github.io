b-carousel(id="carouselSlider"
  style="text-shadow: 1px 1px 2px #333;"
  controls
  indicators
  background="#ababab"
  :interval="9000"
  img-width="1200"
  img-height="900"
  v-model="slide"
  @sliding-start="onSlideStart"
  @sliding-end="onSlideEnd"
)
  b-carousel-slide(v-for="image in images"
    :key="index"
    :img-src="image.url"
    :title="image.title"
   )
